{
  "name": "redux-snake",
  "version": "0.0.1",
  "description": "Little Redux-ish usage for creating a snake game.",
  "main": "dist/index.js",
  "scripts": {
    "buildPackage": "rimraf dist && babel src --out-dir dist",
    "bundleDev": "webpack --config webpack.config.dev.js",
    "bundleProd": "webpack --config webpack.config.prod.js && webpack --env.uglify --config webpack.config.prod.js",
    "buildAll": "npm run buildPackage && npm run bundleDev && npm run bundleProd",
    "startDev": "webpack-dev-server --config webpack.config.dev.js --env.sourceMaps",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/randy-r/redux-snake.git"
  },
  "keywords": [
    "redux",
    "react",
    "snake",
    "game"
  ],
  "author": "randy-r",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/randy-r/redux-snake/issues"
  },
  "homepage": "https://github.com/randy-r/redux-snake#readme",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2016": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "clean-webpack-plugin": "^0.1.16",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.0.1",
    "eslint-loader": "^1.8.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^5.0.1",
    "eslint-plugin-react": "^7.1.0",
    "html-webpack-plugin": "^2.30.1",
    "webpack": "^3.5.6",
    "webpack-dev-server": "^2.7.1",
    "webpack-merge": "^4.1.0"
  },
  "dependencies": {
    "react": "^16.0.0",
    "react-dom": "^16.0.0"
  }
}
